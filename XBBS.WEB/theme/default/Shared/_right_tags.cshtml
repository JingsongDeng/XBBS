<div class='box'>
    <div class='box-header'>
        最新标签
    </div>
    <div class='inner'>
        @{
            if (ViewBag.Targs != null)
            {
                <div class="list_unstyled">
                    @{
                foreach (var item in (List<XBBS.Models.Targs>)ViewBag.Targs)
                {
                    <span class=" tags">
                        <a href="/tag/@item.Title">@item.Title </a>
                    </span>
                }
                    }
                </div>
            }
        }
    </div>
</div>
